    "def normal(x):\n",
    "    marks=['.',',','-',':',';','-']\n",
    "    for i in marks:\n",
    "        x=(str(x).lower()).replace(i,'')\n",
    "    return x   \n",
    "\n",
    "\n",
    "with open('moby.txt','r') as reader, open('moby_clean.txt', 'w') as writer:\n",
    "    l=reader.readlines()\n",
    "    writer.writelines(normal(l))       "
   ]
